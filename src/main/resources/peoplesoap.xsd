<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:tns="http://www.github.com/tbayzin/peopleinfoservice"
           targetNamespace="http://www.github.com/tbayzin/peopleinfoservice"
           elementFormDefault="qualified">

    <xs:element name="getPersonsoapRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="tckn" type="xs:int"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="getPersonsoapResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="personsoap" type="tns:personsoap"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:complexType name="personsoap">
        <xs:sequence>
            <xs:element name="name" type="xs:string"/>
            <xs:element name="surname" type="xs:string"/>
            <xs:element name="city" type="xs:string"/>
            <xs:element name="tckn" type="xs:int"/>
        </xs:sequence>
    </xs:complexType>

</xs:schema>